{
  "name": "react-forms",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && yarn copy",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "api": "rimraf src/api/generated && openapi-generator-cli generate -i ../../processes/order/openapi.json -o src/api/generated -g typescript-angular --global-property models --additional-properties=stringEnums=true,supportsES6=true,npmVersion=10.8.2",
    "copy": "copyfiles -f dist/index.html ../../processes/order/src/main/resources/forms",
    "html2String": "node scripts/html2String.cjs",
    "eslint": "eslint --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mui/material": "^5.16.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tss-react": "^4.9.11"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/js": "^9.8.0",
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@types/copyfiles": "^2",
    "@types/eslint__js": "^8.42.3",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "@vitejs/plugin-react": "^4.3.1",
    "copyfiles": "^2.4.1",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-unused-imports": "^4.0.1",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.3.4",
    "vite-plugin-singlefile": "^2.0.2"
  },
  "packageManager": "yarn@4.3.1"
}
